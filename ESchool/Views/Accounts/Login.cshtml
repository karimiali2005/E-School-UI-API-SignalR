<section class="log-reg-sec1">
    <div class=" title">
        ورود به پنل دانش آموز-مدرسه دکتر حسابی کاشان
    </div>
</section>

<section class=" login-sec2 mt-2">
    <div class=" container">
        @if (!ViewData.ModelState.IsValid)
        {
            <div class="register-fail">
                <span> @Html.ValidationMessage("Error")</span>

            </div>

        }


        <div class=" div-form row mx-0">
            <div class=" col-lg-6 col-12">
                <form asp-action="Login" method="post">
                    <div class="form-group my-4">
                        <label for="" class="mb-3 font-weight-bold">نام کاربری</label>
                        <input type="text" asp-for="@Model.UsersName" class="form-control" id="UsersName" aria-describedby="emailHelp">
                    </div>
                    <div class="form-group my-4">
                        <label for=" " class="mb-3 font-weight-bold">رمز عبور</label>
                        <input type="password" asp-for="@Model.UsersPass" class="form-control" id="UsersPass">
                        <span><img src="~/imagemember/eye.svg" alt=""></span>
                    </div>
                    <div class="forget-rem">
                        <div class="form-group form-check m-0">
                            <input type="checkbox" asp-for="@Model.RememberMe" class="form-check-input" id="remember">
                            <label class="form-check-label" for="remember">به خاطر آوردن</label>
                        </div>
                        <div>
                            <a href="/Account/ForgetPassword" class="text-dark">فراموش کردن رمز عبور / نام کاربری؟</a>
                        </div>
                    </div>
                    <div class="my-4">
                        <button type="submit" class="btn btn-lg text-white submit-enter">ورود</button>
                    </div>
                    <div>
                        <a href="/Account/Register" class=" text-dark">
                            <span class=" ml-1">حساب کاربری ندارید؟</span>
                            <span class=" font-weight-bold">
                                ثبت نام
                                کنید
                            </span>
                        </a>
                    </div>
                </form>
            </div>
            <div class=" col-lg-6 left-img">
                <!-- start cahnge11 -->
                <div><img src="~/imagemember/enter3.png" class=" img-fluid" alt=""></div>
                <!-- end cahnge11 -->

            </div>
        </div>
    </div>
</section>


