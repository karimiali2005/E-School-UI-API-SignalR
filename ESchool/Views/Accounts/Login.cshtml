@section Scripts
{
    <script>
        $('#UsersName').on('keyup', function (event) {
            var arregex = /^[a-zA-Z0-9_ ]*$/;
            if (!arregex.test(event.key)) {
                $('#UsersName').val("");
                alert("اعداد و حروف فقط به صورت انگلیسی وارد شود")
            }
        });
        $('#UsersPass').on('keyup', function (event) {
            var arregex = /^[a-zA-Z0-9_ ]*$/;
            if (!arregex.test(event.key)) {
                $('#UsersPass').val("");
                alert("اعداد و حروف فقط به صورت انگلیسی وارد شود")
            }
        });
    </script>
}