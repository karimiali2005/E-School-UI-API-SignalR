<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <system.webServer>

	  <security>
		  <requestFiltering>
			  <!-- 1 GB -->
			  <requestLimits maxAllowedContentLength="2147483648" />
		  </requestFiltering>
	  </security>    	  
	  
  </system.webServer>
</configuration>